<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:constantSize="false"
    android:dither="true"
    android:variablePadding="false" >

    <item android:state_pressed="true"
        android:drawable="@drawable/quad_button_unpressed" /> <!-- pressed -->
    <item android:state_focused="true"
        android:drawable="@drawable/quad_button_unpressed" /> <!-- focused -->
    <item android:state_hovered="true"
        android:drawable="@drawable/quad_button_unpressed" /> <!-- hovered -->
    <item android:drawable="@drawable/quad_button_unpressed" /> <!-- default -->

</selector>

